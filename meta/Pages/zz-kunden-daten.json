            "name": "$filter",
            "value": "KundenID eq ${parameters.KundenID} AND BetreuerBaseID eq ${parameters.BetreuerBaseID}"
          }
        ],
        "then": [
          {
            "name": "dsoKundenBetreuerBase",
            "type": "set",
            "value": "${result.value[0]}"
          },
