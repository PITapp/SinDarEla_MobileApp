{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "aaKunden",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "allowAdd": true,
                  "allowDelete": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "property": "KundenID",
                      "title": "Kunden ID",
                      "type": "integer"
                    },
                    {
                      "filterProperty": "Base.Name1",
                      "property": "BaseID",
                      "sortProperty": "Base.Name1",
                      "template": "${data.Base?.Name1}",
                      "title": "Base",
                      "type": "string"
                    },
                    {
                      "filterProperty": "KundenStatus.Status",
                      "property": "KundenStatusID",
                      "sortProperty": "KundenStatus.Status",
                      "template": "${data.KundenStatus?.Status}",
                      "title": "Kunden Status",
                      "type": "string"
                    },
                    {
                      "format": "date-time",
                      "formatString": "medium",
                      "property": "Betreuungsbeginn",
                      "title": "Betreuungsbeginn",
                      "type": "string"
                    },
                    {
                      "property": "Vorbemerkungen",
                      "title": "Vorbemerkungen",
                      "type": "string"
                    },
                    {
                      "property": "Info",
                      "title": "Info",
                      "type": "string"
                    }
                  ],
                  "confirmDelete": true,
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "count": "${getKundensCount}",
                  "data": "${getKundensResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-aa-kunden",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Kunden",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbSinDarEla.deleteKunden",
                        "parameters": [
                          {
                            "name": "KundenID",
                            "value": "${event.KundenID}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Kunden deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load Kundens",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbSinDarEla.getKundens",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "KundenKontaktes,Base,KundenStatus"
                          }
                        ],
                        "then": [
                          {
                            "name": "getKundensResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getKundensCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "KundenID",
                            "value": "${event.KundenID}"
                          }
                        ],
                        "path": "edit-aa-kunden",
                        "type": "popup"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        },
        {
          "data": "${getKundensResult.KundenKontaktes}",
          "name": "grid1",
          "type": "grid",
          "confirmDeleteText": "Are you sure you want to delete this record?",
          "confirmDeleteTitle": "Confirmation",
          "confirmDeleteOkText": "Ok",
          "confirmDeleteCancelText": "Cancel",
          "emptyText": "No records to display.",
          "addText": "Add",
          "clearFilterText": "Clear",
          "applyFilterText": "Apply",
          "equalsText": "Equals",
          "notEqualsText": "Not equals",
          "lessThanText": "Less than",
          "lessThanOrEqualsText": "Less than or equals",
          "greaterThanText": "Greater than",
          "greaterThanOrEqualsText": "Greater than or equals",
          "endsWithText": "Ends with",
          "containsText": "Contains",
          "startsWithText": "Starts with"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "aaKunden"
}