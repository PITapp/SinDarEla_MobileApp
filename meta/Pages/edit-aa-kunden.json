  "events": {
    "load": [
      {
        "name": "dbSinDarEla.getKundenByKundenID",
        "parameters": [
          {
            "name": "KundenID",
            "value": "${parameters.KundenID}"
          }
        ],
        "then": [
          {
            "name": "kunden",
            "type": "set",
            "value": "${result}"
          }
        ],
        "error": [],
        "type": "invoke"
      },
