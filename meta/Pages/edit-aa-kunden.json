                                    "type": "back"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Kunden",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbSinDarEla.updateKunden",
                        "parameters": [
                          {
                            "name": "KundenID",
                            "value": "${parameters.KundenID}"
                          },
                          {
                            "name": "Kunden",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  }
                }
