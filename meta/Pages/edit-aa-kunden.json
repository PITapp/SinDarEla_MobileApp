        "name": "dbSinDarEla.getBases",
        "parameters": [],
        "then": [
          {
            "name": "getBasesResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "dbSinDarEla.getKundenStatuses",
        "parameters": [],
        "then": [
          {
            "name": "getKundenStatusesResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
