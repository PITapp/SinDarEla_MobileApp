{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-8",
              "components": [
                {
                  "attributes": [
                    {
                      "name": "(input)",
                      "value": "this.datalistKunden.load();"
                    }
                  ],
                  "name": "textboxSuchenKunden",
                  "placeholder": "Suchen",
                  "style": {
                    "backgroundColor": "#f5f5f5",
                    "float": "left",
                    "fontWeight": "normal",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                }
              ],
              "name": "col2",
              "style": {
                "paddingBottom": "10px",
                "paddingLeft": "10px",
                "paddingRight": "10px",
                "paddingTop": "10px",
                "position": "static"
              },
              "type": "col"
            },
            {
              "class": "col-2",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "code": "this.textboxSuchenKunden.value = null;\nthis.datalistKunden.load();",
                        "type": "execute"
                      }
                    ]
                  },
                  "icon": "clear",
                  "name": "buttonSuchenLoeschen",
                  "style": {
                    "color": "#f5f5f5",
                    "width": "100%"
                  },
                  "text": "",
                  "type": "button"
                }
              ],
              "name": "col3",
              "style": {
                "paddingBottom": "0",
                "paddingLeft": "0",
                "paddingRight": "10px",
                "paddingTop": "10px",
                "position": "static"
              },
              "type": "col"
            },
            {
              "class": "col-2",
              "components": [
                {
                  "icon": "keyboard_hide",
                  "name": "buttonSuchenTastatur",
                  "style": {
                    "color": "#f5f5f5",
                    "width": "100%"
                  },
                  "text": "",
                  "type": "button"
                }
              ],
              "name": "col4",
              "style": {
                "paddingBottom": "0",
                "paddingLeft": "0",
                "paddingRight": "10px",
                "paddingTop": "10px",
                "position": "static"
              },
              "type": "col"
            }
          ],
          "name": "row1",
          "style": {
            "backgroundColor": "#ffc16b",
            "borderBottom": "1px dotted #f1903c",
            "marginBottom": "0",
            "marginLeft": "0",
            "marginRight": "0",
            "marginTop": "0",
            "position": "fixed",
            "width": "100%",
            "zIndex": "999"
          },
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowPaging": false,
                  "count": "${rstKundenCount}",
                  "data": "${rstKunden}",
                  "defaultRowText": "Template for index",
                  "emptyText": "...",
                  "events": {
                    "loadData": [
                      {
                        "name": "dbSinDarEla.getVwKundenUndBetreuerAuswahls",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "BetreuerBaseID eq ${globalBenutzerBaseID} AND (contains(tolower(Name1),tolower('${this.textboxSuchenKunden.value || ''}')) OR contains(tolower(Name2),tolower('${this.textboxSuchenKunden.value || ''}')))"
                          }
                        ],
                        "then": [
                          {
                            "name": "rstKunden",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "rstKundenCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "datalistKunden",
                  "pageSize": 20,
                  "style": {
                    "border": "1px solid rgba(0, 0, 0, 0)"
                  },
                  "template": [
                    {
                      "attributes": [
                        {
                          "name": "(click)",
                          "value": "onClickStartKundenDaten(data)"
                        }
                      ],
                      "components": [
                        {
                          "class": "col-12",
                          "components": [
                            {
                              "name": "image0",
                              "path": "${data.BildURL}",
                              "style": {
                                "borderRadius": "40px",
                                "height": "40px",
                                "left": "25px",
                                "position": "absolute",
                                "top": "15px",
                                "width": "40px"
                              },
                              "type": "image"
                            },
                            {
                              "name": "label0",
                              "style": {
                                "color": "rgb(33, 37, 41)",
                                "display": "block",
                                "fontWeight": "normal"
                              },
                              "text": "${data.Name1} ${data.Name2}",
                              "type": "label"
                            },
                            {
                              "name": "label1",
                              "style": {
                                "color": "#88989b",
                                "display": "block",
                                "fontSize": "14px",
                                "fontWeight": "normal"
                              },
                              "text": "${data.Ort}, ${data.Strasse}",
                              "type": "label"
                            },
                            {
                              "icon": "navigate_next",
                              "name": "icon21",
                              "style": {
                                "color": "#F1903C",
                                "display": "none",
                                "float": "none",
                                "fontSize": "32px",
                                "fontWeight": "normal",
                                "position": "absolute",
                                "right": "30px",
                                "top": "19px"
                              },
                              "type": "icon"
                            }
                          ],
                          "name": "col0",
                          "style": {
                            "paddingLeft": "80px",
                            "paddingTop": "11px",
                            "position": "relative",
                            "verticalAlign": "baseline"
                          },
                          "type": "col"
                        }
                      ],
                      "name": "row0",
                      "style": {
                        "borderBottom": "1px solid #e6ecef",
                        "height": "70px"
                      },
                      "type": "row"
                    }
                  ],
                  "type": "datalist",
                  "wrapItems": false
                }
              ],
              "name": "col5",
              "type": "col"
            }
          ],
          "name": "row2",
          "style": {
            "paddingTop": "55px",
            "position": "static"
          },
          "type": "row"
        },
        {
          "name": "label2",
          "style": {
            "backgroundColor": "#479cc8",
            "color": "#ffffff",
            "marginBottom": "10px",
            "marginTop": "20px",
            "paddingLeft": "10px",
            "width": "100%"
          },
          "text": "Diese Schaltflächen müssen da sein, damit beim Compilieren in der Navigation die Parameter erstellt werden! Sie werden auch als Funktionen genutzt, die über Click-Ereignisse aktiviert werden.\nSiehe app.routes.ts",
          "type": "label",
          "visible": false
        },
        {
          "events": {
            "click": [
              {
                "parameters": [
                  {
                    "name": "KundenID",
                    "value": "0"
                  }
                ],
                "path": "kunden-daten",
                "type": "navigate"
              }
            ]
          },
          "name": "buttonNavigateToKundenDaten",
          "style": {},
          "text": "NavigateTo - KundenDaten",
          "tooltip": "",
          "type": "button",
          "visible": false
        }
      ],
      "container": "contentContainer0",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "globalBenutzerBaseID",
        "type": "set",
        "value": "localStorage.getItem(\"globalBenutzerBaseID\");"
      },
      {
        "code": "this.datalistKunden.load();",
        "type": "execute"
      },
      {
        "name": "onClickStartKundenDaten",
        "type": "set",
        "value": "(data) => {\n    if (this.dialogRef) {\n      this.dialogRef.close();\n    }\n    this.router.navigate(['kunden-daten', data.KundenID]);\n}"
      }
    ]
  },
  "layout": "Kunden",
  "name": "Kunden"
}